    <sql>IF OBJECT_ID('dbo.compra', 'U') IS NOT NULL 
  DROP TABLE dbo.compra; 

IF OBJECT_ID('dbo.producto', 'U') IS NOT NULL 
  DROP TABLE dbo.producto; 

IF OBJECT_ID('dbo.tienda', 'U') IS NOT NULL 
  DROP TABLE dbo.tienda; 

IF OBJECT_ID('dbo.CLIENTE', 'U') IS NOT NULL 
  DROP TABLE dbo.CLIENTE; 

IF OBJECT_ID('dbo.CATEGORIA', 'U') IS NOT NULL 
  DROP TABLE dbo.CATEGORIA; 

IF OBJECT_ID('dbo.DIRECCION', 'U') IS NOT NULL 
  DROP TABLE dbo.DIRECCION; 

IF OBJECT_ID('dbo.CIUDAD', 'U') IS NOT NULL 
  DROP TABLE dbo.CIUDAD; 

IF OBJECT_ID('dbo.PAIS', 'U') IS NOT NULL 
  DROP TABLE dbo.PAIS; 


CREATE TABLE PAIS
(
ID_PAIS INT NOT NULL PRIMARY KEY,
PAIS VARCHAR(50)
);

CREATE TABLE CIUDAD
(
ID_CIUDAD INT NOT NULL PRIMARY KEY,
ID_PAIS INT,
CIUDAD VARCHAR(50)
CONSTRAINT FKID_PAIS FOREIGN KEY(ID_PAIS) REFERENCES PAIS(ID_PAIS)
);

CREATE TABLE DIRECCION
(
ID_DIRECCION INT NOT NULL PRIMARY KEY,
ID_CIUDAD INT,
CALLE VARCHAR(100),
NRO INT,
PISO INT,
CONSTRAINT FKID_CIUDAD FOREIGN KEY(ID_CIUDAD) REFERENCES CIUDAD(ID_CIUDAD)
);


CREATE TABLE CLIENTE
(
ID_CLIENTE INT NOT NULL PRIMARY KEY,
ID_DIRECCION INT,
NOMBRE VARCHAR(50),
FECHANAC DATE,
SEXO CHAR(1),
CONSTRAINT FKID_DIRECCION FOREIGN KEY(ID_DIRECCION) REFERENCES DIRECCION(ID_DIRECCION)
);


CREATE TABLE CATEGORIA
(
ID_CATEGORIA int NOT NULL PRIMARY KEY,
CATEGORIA varchar(100)
);

CREATE TABLE PRODUCTO
(
ID_PRODUCTO INT NOT NULL PRIMARY KEY,
ID_CATEGORIA int,
NOMBRE VARCHAR(100),
DESCRIPCION VARCHAR(100),
MARCA VARCHAR(100),
PRECIO DECIMAL(12,3),
CONSTRAINT FKID_CATEGORIA FOREIGN KEY(ID_CATEGORIA) REFERENCES CATEGORIA(ID_CATEGORIA)
);

CREATE TABLE TIENDA
(
ID_TIENDA INT NOT NULL PRIMARY KEY,
ID_DIRECCION INT,
NOMBRE VARCHAR(100),
TELEFONO VARCHAR(50),
CONSTRAINT FKID_DIRECCION2 FOREIGN KEY(ID_DIRECCION) REFERENCES DIRECCION(ID_DIRECCION)
);

CREATE TABLE COMPRA
(
ID_COMPRA INT NOT NULL PRIMARY KEY,
ID_CLIENTE INT,
ID_TIENDA INT,
ID_PRODUCTO INT,
CANTIDAD INT,
DESCUENTO INT DEFAULT 1,
FECHA DATE,
CONSTRAINT FKID_CLIENTE FOREIGN KEY(ID_CLIENTE) REFERENCES CLIENTE(ID_CLIENTE),
CONSTRAINT FKID_TIENDA FOREIGN KEY(ID_TIENDA) REFERENCES TIENDA(ID_TIENDA),
CONSTRAINT FKID_PRODUCTO FOREIGN KEY(ID_PRODUCTO) REFERENCES PRODUCTO(ID_PRODUCTO)
);



